{
  "name": "fastify-twitch-ebs-tools",
  "version": "2.1.1",
  "description": "Fastify plugin providing useful functions for Twitch Extension Backend Services (EBS)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "clearTestCache": "jest --clearCache",
    "clean": "rimraf dist/*",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint . --ext .ts",
    "compile": "rollup -c",
    "build": "npm run lint && npm run test:cov && npm run clean && npm run compile",
    "dev": "tsc -w --project tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukemnet/fastify-twitch-ebs-tools.git"
  },
  "author": {
    "name": "Łukasz Wójcik",
    "url": "https://www.lukem.net/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukemnet/fastify-twitch-ebs-tools/issues"
  },
  "homepage": "https://github.com/lukemnet/fastify-twitch-ebs-tools",
  "dependencies": {
    "fastify": "^4.7.0",
    "fastify-plugin": "^4.2.1",
    "twitch-ebs-tools": "^2.0.6"
  },
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.8.3",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "esbuild": "^0.15.10",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.1",
    "jest": "^29.1.2",
    "jsonwebtoken": "^8.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.1.0",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "keywords": [
    "fastify",
    "twitch",
    "extension",
    "backend",
    "ebs",
    "twitch-extensions",
    "extension-backend-service",
    "twitch-ext"
  ]
}
